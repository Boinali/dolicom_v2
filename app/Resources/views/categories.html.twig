<!DOCTYPE html>
<html>
<head>
    <title>Gestion des utilisateurs Dolibarr</title>
    {% block stylesheets %}
        <link rel="stylesheet"
              href="{{ asset('assets/css/test.css') }}" type="text/css" />
        <link rel="stylesheet"
              href="{{ asset('assets/css/bootstrap.css') }}">
        <link rel="stylesheet"
              href="{{ asset('assets/css/font-awesome.css') }}">

    {% endblock %}
</head>
<body>
<div class="container">
    <div class="row">
        <div class="col-lg-4">
            <h1>Gestion des Cat√©gories</h1>
            <div class="well">
                <form method="post" {{ form_enctype(formCreateCat) }}>
                    {{ form_widget(formCreateCat) }}
                </form>
            </div>
            {#{{ form_start(formCreateCat) }}#}
                {##}
            {#{{ form_end(formCreateCat) }}#}
        </div>
    </div>
    <div class="row">
        <div class="col-lg-offset-4 col-lg-4">
            <h1>Gestion des Services</h1>
            <div class="well">
                <form method="post" {{ form_enctype(formCreateServ) }}
                {#{{ form_start(formCreateServ) }}#}
                {{ form_widget(formCreateServ) }}
                {#{{ form_end(formCreateServ) }}#}
            </div>
        </div>
        <br>
        {{ Erreur|default('-') }}
    </div>
</div>
</body>
<script src="{{ asset('js/jquery.js') }}"></script>
<script src="{{ asset('js/bootstrap.js') }}"></script>
</html>
